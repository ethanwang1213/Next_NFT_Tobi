{
  "name": "tobiratory-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ]
  },
  "scripts": {
    "journal": "cd apps/journal && npm run dev",
    "app2": "cd apps/app2 && npm run dev",
    "emulate:flow": "cd apps/flow && npm run emulator",
    "deploy:flow": "cd apps/flow && npm run deploy",
    "mint:flow": "cd apps/flow && npm run mint",
    "deploy-testnet:flow": "cd apps/flow && npm run deploy-testnet",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "turbo": "latest"
  },
  "packageManager": "npm@8.11.0"
}
