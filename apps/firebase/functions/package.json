{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts . --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only hosting,auth,functions,pubsub,firestore,storage",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run deploy-dev",
    "deploy-dev": "firebase deploy -P tobiratory-f6ae1 --only hosting,functions,firestore,storage",
    "deploy-stg": "firebase deploy -P tobiratory-stg --only hosting,functions,firestore,storage",
    "deploy-prd": "firebase deploy -P tobiratory --only functions:shopifyOrders-handleOrdersPaid",
    "logs": "firebase functions:log",
    "test": "jest"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^3.5.2",
    "@onflow/fcl": "^1.6.0",
    "@sendgrid/mail": "^7.7.0",
    "cors": "^2.8.5",
    "elliptic": "^6.5.4",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^4.4.1",
    "js-sha256": "^0.10.1",
    "node-fetch": "^2.6.11",
    "sha3": "^2.1.4",
    "voucher-code-generator": "^1.3.0"
  },
  "devDependencies": {
    "@onflow/typedefs": "^1.1.1",
    "@types/elliptic": "^6.4.16",
    "@types/jest": "^29.5.2",
    "@types/node-fetch": "^2.6.4",
    "@types/sinon": "^10.0.15",
    "@types/voucher-code-generator": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "csv-parse": "^5.4.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.3",
    "jest": "^29.5.0",
    "sinon": "^15.1.0",
    "ts-jest": "^29.1.0",
    "types": "*",
    "typescript": "^4.5.4"
  },
  "private": true
}
