{
  "compilerOptions": {
    "sourceMap": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve"
  },
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "global.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
  ],
  "exclude": [
    "node_modules"
  ]
}
