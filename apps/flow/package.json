  "scripts": {
    "emulator": "flow emulator",
    "deploy": "flow project deploy --network=emulator",
    "createAccount": "flow accounts create --key 2b4befc2504c1c89e53ef15d743abde0bf2c6aafad57aad27f30a321f88bd17a93737f0beb04b7650fee0c9242a3110b62631bbb7607e210673a0cceb8d59c77",
    "mint": "flow transactions send ./cadence/transactions/mintNFT.cdc --network=emulator 'title' 'some text' 'https://aaaa'",
    "mint2": "flow transactions send ./cadence/transactions/mintNFT.cdc --network=emulator --signer emulator-account-2 'title 2' 'some text 2' 'https://bbbb'",
    "getIds": "flow scripts execute ./cadence/scripts/getIds.cdc --network=emulator f8d6e0586b0a20c7",
    "getIds2": "flow scripts execute ./cadence/scripts/getIds.cdc --network=emulator 01cf0e2f2f715450",
    "getMetadata": "flow scripts execute ./cadence/scripts/getMetadata.cdc --network=emulator f8d6e0586b0a20c7 0",
    "transfer": "flow transactions send ./cadence/transactions/transferNFT.cdc --network=emulator 01cf0e2f2f715450 0",
    "deploy-testnet": "flow project deploy --network=testnet",
    "test": "flow project test --network=emulator"
  },
  "dependencies": {
    "@onflow/fcl": "^1.3.2"
  }
